pipeline {
  agent {
    label 'kaniko-label'
  }
  stages {
    stage('Build image') {
      steps {
        container('kaniko') {
          sh '/kaniko/executor --cache  --cache-repo=andrezug/cache  --dockerfile `pwd`/Dockerfile --context `pwd` --destination=andrezug/testkaniko:v0.7'
        }
      }
    }
  }
}